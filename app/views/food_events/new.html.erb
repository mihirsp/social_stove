<%= form_for :food_event, url: food_events_path do |f| %>
  <p>
    <%= f.label :title %><br>
    <%= f.text_field :title %>
  </p>
 
  <p>
    <%= f.label :description %><br>
    <%= f.text_area :description %>
  </p>
 
 <p>
    <%= f.label :meal_type %><br>
    <%= f.text_field :meal_type %>
  </p>

  <p>
    <%= f.submit %>
  </p>
<% end %>